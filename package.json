{
  "name": "data-preparation",
  "version": "0.1.0",
  "description": "data-preparation",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "start": "node -r dotenv/config build/index.js",
    "cleanup": "rm -rf dist template jest.config.js jest.config.js.map node_modules",
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "fix:eslint": "eslint --fix \"**/*.ts\"",
    "fix:prettier": "prettier --write \"**/*.ts\"",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"**/*.ts\"",
    "lint:prettier": "prettier --check \"**/*.ts\"",
    "prepare": "husky install",
    "test": "jest --config=jest.config.ts --passWithNoTests",
    "test:watch": "jest --config=jest.config.ts --watch"
  },
  "keywords": [
    "mojaloop",
    "actio",
    "frm"
  ],
  "contributors": [
    {
      "name": "Len Bekker"
    },
    {
      "name": "Jean-Pierre Nell"
    }
  ],
  "license": "ISC",
  "dependencies": {
    "@frmscoe/frms-coe-lib": "^0.2.1",
    "@frmscoe/frms-coe-startup-lib": "^0.0.14",
    "@log4js-node/logstash-http": "^1.1.0",
    "@types/koa": "^2.13.7",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-router": "^7.4.4",
    "arangojs": "^8.2.1",
    "dotenv": "^16.0.3",
    "elastic-apm-node": "^3.44.1",
    "ioredis": "^5.3.2",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.1",
    "koa-helmet": "^7.0.2",
    "koa-router": "^12.0.0",
    "node-cache": "^5.1.2",
    "tslib": "^2.6.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "8.0.3",
    "ioredis-mock": "^8.7.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
